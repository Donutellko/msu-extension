{
    "name": "MSU Extension (my small utility)",
    "version": "1.1",
    "manifest_version": 2,

	"browser_action": {
		"default_title": "Make my life a bit easier. Please."
	},
	"content_scripts": [
		{
			"matches": ["*://*.spbstu.ru/*"],
			"js": ["spbstu/autologin.js"],
			"run_at": "document_idle",
			"all_frames": true
		}, {
			"matches": ["*://cas.spbstu.ru/login*"],
			"js": ["spbstu/autologin_cas.js"],
			"run_at": "document_idle",
			"all_frames": true
		}
	],
	"permissions": [
		"activeTab",
		"webNavigation",
		"storage"
	]
 }